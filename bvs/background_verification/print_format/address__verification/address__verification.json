 "html": "<div style=\"height: 0.5in\"></div>\n<div class=\"container\">\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            {% if letter_head and not no_letterhead -%}\n            <div class=\"letter-head\">{{ letter_head }}</div>\n            {%- endif %}<br><br>\n            <h3><b><u>\n                        <center>Address Verification</center>\n                    </u></b></h3>\n            <br>\n            <div align=\"left\">Dear Sir/ Madam,</div><br>\n            <div align=\"left\">The below mentioned information confirmed with\n                Mr./Ms...................................... on.....................................\n                <br>\n            </div>\n        </div>\n    </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <!-- <table class=\"table table-bordered\"> -->\n            <table class=\"table table-bordered\" border=\"1\">\n                <tr>\n                    <td colspan=\"4\"><b>Candidate VHRS Ref.No.</b></td>\n                    <td colspan=\"8\">{{doc.ref_id}}</td>\n                </tr>\n                <tr>\n                    <td colspan=\"4\"><b>Candidate Name</b></td>\n                    <td colspan=\"8\">{{doc.candidate_name}}</td>\n                </tr>\n                <tr>\n                    <td colspan=\"4\"><b>Address</b></td>\n                    <td colspan=\"8\">{{doc.address_line1}},{{doc.address_line2}},{{doc.address_line3 or ''}}-{{doc.pincode}}</td>\n                </tr>\n                <tr>\n                    <td colspan=\"4\"><b>Contact No./Landmark</b></td>\n                    <td colspan=\"8\">{{doc.contact_number}}</td>\n                </tr>\n                <tr>\n                    <td colspan=\"4\"><b>Type of Address</b></td>\n                    <td colspan=\"8\"> &nbsp;&nbsp;&emsp;&emsp;&emsp;&emsp;Present\n                        &nbsp;&nbsp;&nbsp;&nbsp;&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;Permanent</td>\n                </tr>\n                <tr>\n                    <td colspan=\"4\"><b>Nature Of Residence</b></td>\n                    <td colspan=\"8\">Own House &emsp;&emsp;Rented &emsp;&emsp;\n                        Paying Guest &emsp;&emsp;Hostel</td>\n                </tr>\n                <tr>\n                    <td colspan=\"4\"><b>Period Of Stay</b></td>\n                    <td colspan=\"8\"></td>\n                </tr>\n                <tr>\n                    <td colspan=\"4\"><b>Nearby Police Station</b></td>\n                    <td colspan=\"8\"></td>\n                </tr>\n                <tr>\n                    <td colspan=\"4\"><b>Verifier's(Neighbor)Name</b></td>\n                    <td colspan=\"8\"></td>\n                </tr>\n                <tr>\n                    <td colspan=\"4\"><b>Relationship with Candidate </b></td>\n                    <td colspan=\"8\"></td>\n                </tr>\n                <tr>\n                    <td colspan=\"4\"><b>Verifier Signature</b></td>\n                    <td colspan=\"8\"></td>\n                </tr>\n                <tr>\n                    <td colspan=\"4\"><b>Remarks</b></td>\n                    <td colspan=\"8\"></td>\n                </tr>\n            </table>\n            <br><br>\n        </div>\n    </div>\n    <div class=\"row\">\n        <br><br>\n        <div class=\"col-xs-7\"><b>Field Executive Signature</b></div>\n        <div class=\"col-xs-5\"><b>Quality Reviewer Signature</b></div>\n\n    </div>\n\n\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n\n            <div id=\"footer-html\" class=\"visible-pdf\"\n                style=\"display: block !important; position: absolute;\tbottom: 0.75in;\">\n                <div class=\"letter-head-footer\">\n                    <div style=\"text-align: center;\">{{ footer }}</div>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div style=\"height: 0.5in\"></div>\n</div>",
